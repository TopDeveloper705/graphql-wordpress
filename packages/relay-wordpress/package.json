  "scripts": {
    "dev": "npm run persist-queries && GQL_HOST=http://localhost:8080 kyt dev -C kyt.dev.js",
    "prod": "GQL_HOST=http://localhost:8080 npm run build && npm run persist-queries && npm run start",
    "build": "kyt build -C kyt.prod.js",
    "start": "forever start build/server/main.js",
    "relay": "relay-compiler --src ./src --schema ./tools/schema.graphql && npm run clear-cache",
    "persist-queries": "babel-node ./tools/persistedQueries",
    "clear-cache": "redis-cli FLUSHALL && npm run persist-queries",
    "prettier": "prettier-eslint \"src/**/*.js\" --ignore \"**/__generated__/**\" --write"
  },
