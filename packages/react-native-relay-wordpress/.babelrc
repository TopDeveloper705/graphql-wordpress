{
  "presets": [
    "babel-preset-expo",
    "flow"
  ],
  "plugins": [
    "relay",
    "transform-flow-strip-types",
    "transform-class-properties",
    ["module-resolver", {
      "root": ["./src", "./types"]
    }]
  ],
  "env": {
    "development": {
      "plugins": ["transform-react-jsx-source"]
    }
  }
}
