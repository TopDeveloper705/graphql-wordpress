{
  "name": "@wonderboymusic/apollo-wordpress",
  "version": "0.0.2-13",
  "description": "Headless WordPress isomorphic Apollo app",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/staylor/graphql-wordpress/packages/apollo-wordpress"
  },
  "author": "Scott Taylor <scott.c.taylor@mac.com>",
  "license": "MIT",
  "dependencies": {
    "@wonderboymusic/graphql-wordpress-components": "^0.0.2-13",
    "base-64": "^0.1.0",
    "classnames": "^2.2.5",
    "compression": "^1.7.0",
    "debounce": "^1.1.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "farce": "^0.2.2",
    "forever": "^0.15.3",
    "found": "^0.3.4",
    "graphql-tag": "^2.5.0",
    "http-proxy-middleware": "^0.17.4",
    "isomorphic-fetch": "^2.2.1",
    "kyt": "^0.9.0",
    "md5": "^2.2.1",
    "morgan": "^1.8.2",
    "path-to-regexp": "1.7.0",
    "persistgraphql": "^0.3.11",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-apollo": "^1.4.15",
    "react-cookie": "^2.1.1",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.4.0",
    "redis": "^2.8.0",
    "serialize-javascript": "^1.4.0",
    "universal-cookie-express": "^2.1.0"
  },
  "scripts": {
    "dev": "GQL_HOST=http://localhost:8080 kyt dev -C kyt.dev.js",
    "prod": "GQL_HOST=http://localhost:8080 npm run build && npm run start",
    "build": "kyt build -C kyt.prod.js",
    "start": "forever start build/server/main.js",
    "persist-queries": "persistgraphql src src/apollo/queries.json --add_typename",
    "prettier": "prettier-eslint \"src/**/*.js\" --ignore \"src/public/**\" --write",
    "types-ts": "apollo-codegen generate src/graphql/**/*.graphql --schema tools/schema.json --target typescript --output src/types/index.ts",
    "types-flow": "apollo-codegen generate src/graphql/**/*.graphql --schema tools/schema.json --target flow --output src/types/index.js"
  },
  "devDependencies": {
    "apollo-codegen": "^0.17.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-plugin-module-resolver": "^3.0.0-beta",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-kyt-react": "^0.3.0",
    "bluebird": "^3.5.0",
    "enzyme": "^3.1.0",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-kyt": "^0.5.0",
    "eslint-config-prettier": "^2.5.0",
    "eslint-import-resolver-babel-module": "^4.0.0-beta.5",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.3.0",
    "prettier": "^1.7.0",
    "prettier-eslint": "^8.2.1",
    "prettier-eslint-cli": "^4.3.2",
    "react-test-renderer": "^16.0.0"
  }
}
