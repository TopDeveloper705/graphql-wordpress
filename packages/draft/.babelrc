{
  "plugins": [
    ["module-resolver", {
      "root": ["./src"]
    }]
  ],
  "env": {
    "app": {
      "presets": [
        ["kyt-react", {"modules": true}],
        "flow"
      ],
      "plugins": [
        "emotion",
        "transform-decorators-legacy",
        "transform-class-properties",
        "transform-object-rest-spread",
        ["module-resolver", {
          "root": ["./src"]
        }]
      ]
    },
    "graphql": {
      "presets": [
        ["kyt-core", {"modules": true}],
        "flow"
      ],
      "plugins": [
        "transform-class-properties",
        "transform-es2015-modules-commonjs",
        "transform-object-rest-spread",
        "transform-runtime",
        ["module-resolver", {
          "root": ["./src"]
        }]
      ]
    }
  }
}
