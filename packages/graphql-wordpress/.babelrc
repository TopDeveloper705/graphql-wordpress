{
  "presets": [
    "kyt-core"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-es2015-modules-commonjs",
    "transform-object-rest-spread",
    "transform-regenerator",
    ["module-resolver", {
      "root": ["./"],
      "alias": {
        "jest": "./jest",
        "data": "./src/data",
        "schema": "./src/schema",
        "type": "./src/schema/type",
        "connection": "./src/schema/connection",
        "enum": "./src/schema/enum",
        "interface": "./src/schema/interface",
        "query": "./src/schema/query",
        "mutation": "./src/schema/mutation",
        "field": "./src/schema/field"
      }
    }]
  ]
}
